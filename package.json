{
  "name": "vue-modal-manager",
  "description": "Modal manager for Vue 3 applications. Easily open, close and manage modals. Create custom reusable modals.",
  "keywords": [
    "vue",
    "modal",
    "manager",
    "modal-manager",
    "vue-modal-manager",
    "vue modal manager",
    "vue modal",
    "provider",
    "modal-provider",
    "vue-provider"
  ],
  "version": "0.0.7",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/vue-modal-manager.cjs",
  "module": "./dist/vue-modal-manager.esm.js",
  "sideEffects": false,
  "homepage": "https://vue-modal-manager.netlify.app",
  "author": "Mirjalol Norqulov",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/vue-modal-manager.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "radix-vue": "^0.4.1",
    "uuid": "^9.0.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.17.17",
    "@types/uuid": "^9.0.5",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "naive-ui": "^2.35.0",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0",
    "vitepress": "1.0.0-rc.20",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.11"
  }
}
